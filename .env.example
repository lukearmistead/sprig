# Sprig Configuration
APP_ID=your_teller_app_id_here
ENVIRONMENT=development

# Account access tokens separated by commas
ACCESS_TOKENS=bank_1_access_token_here,bank_2_access_token_here

# Claude API key for transaction categorization
CLAUDE_API_KEY=sk-ant-api03-your_claude_api_key_here

# Certificate paths (required for development and production environments)
CERT_PATH=certs/certificate.pem
KEY_PATH=certs/private_key.pem

# Database file path
DATABASE_PATH=sprig.db

# Logging configuration (optional)
# LOG_LEVEL options: DEBUG, INFO, WARNING, ERROR (default: INFO)
LOG_LEVEL=INFO
# LOG_FILE: Optional file path for logging output (omit for console-only logging)
# LOG_FILE=sprig.log
